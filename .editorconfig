;.editorconfig

root = true

[**.js]
indent_style = space
indent_size = 4
keep_array_indentation = 1
insert_final_newline = true

[**.json]
indent_style = space
indent_size = 4

[**.jsx]
e4x = true
indent_style = space
indent_size = 4

[**.css]
indent_style = space
indent_size = 4

[**.html]
indent_style = space
indent_size = 4
max_char = 78
brace_style = expand
