            endif
        else
            " ÂÖºÂÆπwindowsÁöÑÁéØÂ¢ÉÂèòÈáè$VIM
            let $VIM = $HOME."/.vim"
            set shell=/bin/sh
            " adapt gvim $VIMRC
            let $VIMRC=$MYVIMRC
        endif
