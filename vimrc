    " Identify platform {
        silent function! OSX()
            return system('uname')=~'Darwin'
        endfunction
        silent function! LINUX()
            return has('unix') && !has('macunix') && !has('win32unix')
        endfunction
        silent function! WINDOWS()
            return  (has('win16') || has('win32') || has('win64'))
        endfunction
    " }

    " Windows Compatible {
        if WINDOWS()
