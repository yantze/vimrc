                set statusline  =%<%f\ %h%m%r%=%{(&fenc==\"\"?&enc:&fenc).((exists(\"+bomb\")\ &&\ &bomb)?\",B\":\"\")}
                set statusline +=%{\"[\".&ff.\"]\"} "file format
                set statusline +=%k
